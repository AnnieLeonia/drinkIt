<!DOCTYPE HTML>
<html>
  <head>
    <title><%= title %></title>
  </head>

  <body>
    <h1>Welcome to <%= title %>!</h1>

    <ul>
        <% drinks.forEach(function(drink) { %>
            <li>
                <%= drink.name %>
            </li>
        <% }) %>
    </ul>

  </body>
  <script>
      var drinks = <%- JSON.stringify(drinks) %>
      console.log(drinks);
  </script>
</html>